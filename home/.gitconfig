### included .gitconfig.secrets file should have the following:
# [user]
  # name = FIRSTNAME LASTNAME
  # email = USER@EXAMPLE.com
# [github]
  # user = USER
  # token = TOKEN
# [gist-diff]
	# token = TOKEN
###
[include]
  path = ~/.gitconfig.secrets
###

[core]
  # autocrlf = input
  whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
  eol = lf
  pager = less
	excludesfile = /Users/kswedberg/.gitignore_global
  # editor = subl -n -w

[init]
  templatedir = ~/.git_template

[push]
	default = current

[alias]
  a = add --all
  all = add --all
  br = branch
  ci = commit
  cia = commit --allow-empty
  co = checkout
  df = diff
  dt = difftool --no-prompt
  fetchp = fetch --prune
  g = grep -I
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%C(bold blue)<%an>%Creset' --abbrev-commit
  logl = log --oneline --decorate
  pp = !"echo 'Pull'; git pull --rebase; echo 'Push'; git push"
  pusht = !"git push; git push --tags"
  ms = merge --squash
  msn = merge --squash --no-commit
  rv = review
  st = status
  rename = branch -m
  t = tag
  # Tags are sorted  by version:refname per [tag] config below
  tagl = tag -l
  taglt = !"git tag -l | tail"
  su = submodule update --init
  push-to = !"git-push-to.sh"
  wtf = !"git-wtf.sh"
  syncupstream = !"git fetch upstream; git checkout master; git merge upstream/master; git push; echo 'Check out your branch and run git rebase master. See: http://www.xanthir.com/b4hf0'"

  ## The following aliases require git-extras (http://github.com/visionmedia/git-extras)
  delete = delete-branch
  del = delete-branch

[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan

[apply]
  # whitespace = nowarn
[merge]
	tool = sourcetree
[mergetool]
  prompt = false
# [mergetool "extMerge"]
#   cmd = extMerge "$BASE" "$LOCAL" "$REMOTE" "$MERGED"
#   trustExitCode = true
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[branch]
	autosetuprebase = always
[pull]
	rebase = true
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/kswedberg/.stCommitMsg
