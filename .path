
######## PATHS: EARLIER in path takes precedence ######


# MAMP (if running rails app that requires mysql2 lib, put this before Homebrew libraries path)
PATH="/Applications/MAMP/Library/bin:/Applications/MAMP/bin/php/php5.3.14/lib/php:/Applications/MAMP/bin/php/php5.3.14/bin:$PATH"

PATH="$(brew --prefix josegonzalez/php/php54)/bin:$PATH"
# Homebrew libraries
PATH=/usr/local/bin:/usr/local/sbin:$PATH

# DRUPAL
# PATH="$PATH:$HOME/Sites/drush"

# FONT CONVERTER STUFF
# PATH="$PATH:$HOME/bin/ttf2eot:$HOME/bin/font-converter"

#RBENV
PATH="$HOME/.rbenv/bin:$PATH"

# MY PATHS
PATH="$PATH:$HOME/bin:$HOME/bin/tfs:$HOME/dotfiles"

# COREUTILS added via homebrew...
PATH="/usr/local/opt/coreutils/libexec/gnubin:$PATH"
MANPATH="/usr/local/opt/coreutils/libexec/gnuman:$MANPATH"

export PATH
